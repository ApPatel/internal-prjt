
<div class="listheading"><div class="fontstyling"><h2>Sign up</h2></div></div>
<div class="wrapper2">
<%= form_for(resource, :as => resource_name, :url => registration_path(resource_name)) do |f| %>
  <%= devise_error_messages! %>
   
  <table><tr><td><%= f.label :email %></td>
  <td><%= f.email_field :email %></td></tr>

  <tr><td><%= f.label :password %></td>
  <td><%= f.password_field :password %></td></tr>

  <tr><td><%= f.label :password_confirmation %></td>
  <td><%= f.password_field :password_confirmation %></td></tr>
  <tr><td><%= f.label :'Profile picture'%>
  	<td><%= f.file_field :image %></td><tr></table>
  <div class="signin"><%= f.submit "Sign up" %><br /><br />
<% end %>

<%= render "devise/shared/links" %>
</div>
</div>